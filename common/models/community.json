{
  "name": "community",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp" : {
      "createdAt" : "createdAt",
      "updatedAt" : "updatedAt",
      "required" : false
    }
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "bio_text": {
      "type": "string"
    },
    "bio_image": {
      "type": "string"
    },
    "bio_video": {
      "type": "string"
    },
    "privacy": {
      "type": "string"
    },
    "hasBilling": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "members": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "sourceId",
      "through": "community_has_member",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
